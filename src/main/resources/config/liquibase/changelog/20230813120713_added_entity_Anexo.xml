<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <!--
        Added the entity Anexo.
    -->
    <changeSet id="20230813120713-1" author="jhipster">
        <createTable tableName="anexo">
            <column name="id" type="bigint" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="nome_arquivo" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="nome_original" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="extensao" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="criado_em" type="${datetimeType}">
                <constraints nullable="true" />
            </column>
            <column name="atualizado_em" type="${datetimeType}">
                <constraints nullable="true" />
            </column>
            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here -->
        </createTable>
        <dropDefaultValue tableName="anexo" columnName="criado_em" columnDataType="${datetimeType}"/>
        <dropDefaultValue tableName="anexo" columnName="atualizado_em" columnDataType="${datetimeType}"/>
    </changeSet>
</databaseChangeLog>
